# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[alias]
        co = checkout
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = nvim
	ignorecase = false
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[difftool]
        prompt = false
[help]
        autocorrect = 1
[gpg]
	program = /usr/local/MacGPG2/bin/gpg2
